<div class="author">
    <h1 class="author__heading">Search Result</h1>
</div>
<section class="article-ul">
    <% data.forEach(post => { %> 
        <li> 
            <a href="/post/<%= post._id  %>">
                <span class="article-title"><%= post.title %></span>
                <span class="article-list__date"><%=post.createdAt.toDateString()%></span>
            </a>
        </li>
        <% }) %>
</section>